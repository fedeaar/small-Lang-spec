{
	"name": "smallLang-spec-theme-natanvek",
	"type": "dark",
	"include": "./sc/sc.dark_plus.json",
	"colors": {
		"editor.foreground": "#ced5d0"
	},
	"tokenColors": [
		{
			"name": "proc / pred / aux / pre / post / enum / type / wp",
			"scope": [
				"keyword.control.slspec"
			],
			"settings":{
				"foreground": "#358cd6",
				"fontStyle": "italic"
			}
		},
		{
			"name": "if  / fi / endif / while / endwhile / skip / :=",
			"scope": [
				"keyword.function.slspec"
			],
			"settings":{
				"fontStyle": "italic",
				"foreground": "#c586c0"
			}
		},
		{
			"name": "∑ | sum / ∏ | prod",
			"scope": [
				"support.function.slspec"
			],
			"settings":{
				"fontStyle": "italic bold",
				"foreground": "#ffa13d"
			}
		},
		{
			"name": "then  / else / do",
			"scope": [
				"keyword.function.inner.slspec"
			],
			"settings":{
				"fontStyle": "",
				"foreground": "#82d8ee"
			}
		},
		{
			"name": "from / to",
			"scope": [
				"support.function.inner.slspec"
			],
			"settings":{
				"fontStyle": "",
				"foreground": "#82d8ee"
			}
		},
		{
			"name": "in / out / inout",
			"scope": [
				"variable.parameter.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "ℤ / ℝ / Char / Bool / T / ⨉ / ",
			"scope": [
				"entity.name.type.slspec",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ffa13d"
			}
		},
		{
			"name": "seq[] / mat[]",
			"scope": [
				"entity.name.type.seq.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "true / false / ⊥",
			"scope": [
				"constant.language.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "nombre de funciones",
			"scope": [
				"function.name.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#dcdcaa"
			}
		},
		{
			"name": "carácteres",
			"scope": [
				"constant.character.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#CE9178"
			}
		},
		{
			"name": "operadores lógicos",
			"scope": [
				"keyword.operator.logic.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ff6969"
			}
		},
		{
			"name": "operadores matemáticos",
			"scope": [
				"keyword.operator.math.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#4ec9b0"
			}
		},
		{
			"name": "parentesis",
			"scope": [
				"punctuation.parenthesis.slspec"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ec96af"
			}
		},
		{
			"name": "comentario multilínea",
			"scope": [
				"comment.block.slspec"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "comentario de línea",
			"scope": [
				"comment.line.slspec"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		
		{
			"name": "scheme selector $$",
			"scope": [
				"keyword.control.slconfig"
			],
			"settings":{
				"foreground": "#299e12",
				"fontStyle": ""
			}
		},
		{
			"name": "delimitadores",
			"scope": [
				"keyword.operator.slconfig"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#2088b8"
			}
		}
	]
}
